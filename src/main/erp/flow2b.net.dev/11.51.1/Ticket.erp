extend entity Ticket {
  extend view form = extend("nav") {
    x{
      button("To Incident", visible = ui.edit) {
        let d = description
        let r = convertTo(Incident)
        r.desc = d
      }
      button("To Tech Task", visible = ui.edit) {
        let d = description
        let r = convertTo(TechTask)
        r.desc = d
      }
    }
  }
}